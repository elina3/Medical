/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.navigator-hover {
    color:blue;
}
/** 自定义其他点击态样式类 **/
.other-navigator-hover {
    color:red;
}

.frame-container{
    height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 10rpx 0;
  box-sizing: border-box;
  position: relative;
}

text.developing{
    display: block;
    width: 100%;
    text-align: center;
    height: 100%;
    padding: 450rpx 0;
    color: #999;
    font-size: 30rpx;
}
.fix-line{
    border-top: 1px solid #f2f2f2;
    width: 100%;
    height:1px;
    background: #fff;
    position: fixed;
    bottom: 0px;
}
